        <Button Content="Button" HorizontalAlignment="Left" Height="45" Margin="420,131,0,0" VerticalAlignment="Top" Width="52" Click="Button_Click_1"/>
        <Button Content="Button" HorizontalAlignment="Left" Height="45" Margin="580,131,0,0" VerticalAlignment="Top" Width="52" ButtonBase.Click="Button_Click_2"/>

